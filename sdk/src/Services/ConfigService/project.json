{
  "version": "1.0.0-*",
  "description": "",
  "authors": [ "" ],

  "dependencies": {
    "Core": "1.0.0-*"
  },

  "buildOptions": {
    "outputName": "AWSSDK.ConfigService",
    "define": [ "CORECLR", "AWS_ASYNC_API" ],
    "compile": {
      "exclude": [
        "**/_pcl/*",
        "**/_pcl-sl/*",
        "**/_bcl35/*",
        "**/_bcl45/*",
        "**/_bcl/*",
        "**/_bcl+sl/*",
        "**/_bcl+unity/*",
        "**/_unity/*",
        "**/_android/*",
        "**/_ios/*",
        "**/_pclReference/*",
        "**/_win8/*",
        "**/_winPhone81/*",
        "**/_winPhoneSilverlight8/*"
      ]
    }
  },


  "frameworks": {
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "ADD_SUPPORT_IORDERED_DICTIONARY", "ADD_SUPPORT_ICLONEABLE" ]
      },
      "dependencies": {
        "System.Collections": "4.0.11-rc2-24027",
        "System.Linq": "4.1.0-rc2-24027",
        "Microsoft.CSharp": "4.0.1-rc2-24027"
      }
    }
  }
}